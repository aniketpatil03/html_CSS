<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Create Challenging GRID</title>

    <style>
      h5{
        color: blue;
        text-align: center;
        font-size: 1em;
      }

     .parent {
       display: grid;
       grid-template-rows: 5em 5em 5em 5em;
       grid-template-columns: 33% 33% 33%;
      }
      .one{
        background-color: yellow;
        grid-column-start:1;
        grid-column-end:3;
      }
      .two{
        background-color: cyan;
        grid-row-start: 1;
        grid-row-end: 4;

        grid-column-start: 3;
        grid-column-end: 4;
        /* Above two lines if starting on diff. 
        column then desired */
      }
      .three{
        background-color: gold;
        grid-row-start: 2;
        grid-row-end: 5;

        grid-column-start: 1;
        grid-column-end: 2;
      }
      .four{
        background-color: brown;
      }
      .five{
        background-color: red;
      }
      .six {
        background-color: orange;
        grid-column-start: 2;
        grid-column-end: 4;
      }
    </style>
  </head>
  <body>
    <h5>CSS Grids</h5>
    <div class="parent">
      <div class="one">1</div>
      <div class="two">2</div>
      <div class="three">3</div>
      <div class="four">4</div>
      <div class="five">5</div>
      <div class="six">6</div>
    </div>
    </ul>
  </body>
</html>
